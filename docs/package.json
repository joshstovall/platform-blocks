{
  "name": "@platform-blocks/docs",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "docs:generate": "node -e \"console.log('docs:generate skipped (legacy unified removed)')\"",
    "demos:generate": "npx --yes tsx ../scripts/generate-demos.ts",
    "demos:validate": "npx --yes tsx ../scripts/validate-demos.ts",
    "postinstall": "npm run demos:generate && (npm run demos:validate || true) && npm run generate-code-map",
    "create-demo": "tsx scripts/create-demo.ts",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:demos": "eslint examples/components --ext .tsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "generate-code-map": "npx --yes tsx scripts/generate-static-code-map.ts",
    "build-web": "expo export --platform web",
    "preview-build": "npm run build-web && npx serve dist"
  },
  "dependencies": {
    "@babel/preset-flow": "^7.27.1",
    "@babel/runtime": "^7.28.3",
    "@expo/metro-runtime": "~6.1.1",
    "@platform-blocks/charts": "file:../charts",
    "@platform-blocks/ui": "file:../ui",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.4.7",
    "expo": "^54.0.10",
    "expo-audio": "~1.0.13",
    "expo-clipboard": "~8.0.7",
    "expo-document-picker": "~14.0.7",
    "expo-haptics": "~15.0.7",
    "expo-router": "~6.0.10",
    "expo-status-bar": "~3.0.6",
    "expo-video-thumbnails": "~10.0.7",
    "glob": "^11.0.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "nodemon": "^3.1.10",
    "react-refresh": "^0.17.0",
    "serve": "^14.2.4",
    "tsx": "^4.20.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
