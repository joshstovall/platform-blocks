{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "platform-blocks": [
        "../ui/src/index.ts"
      ],
      "platform-blocks/*": [
        "../ui/src/*"
      ],
      "@platform-blocks/ui": [
        "../ui/src/index.ts"
      ],
      "@platform-blocks/ui/*": [
        "../ui/src/*"
      ],
      "@platform-blocks/charts": [
        "../charts/src/index.ts"
      ],
      "@platform-blocks/charts/*": [
        "../charts/src/*"
      ],
      "@platform-blocks/charts-types": [
        "../charts/src/types.ts"
      ]
    },
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "components/**/*",
    "screens/**/*",
    "utils/**/*",
    "hooks/**/*",
    "data/**/*",
    "app/**/*",
    "index.ts",
    "index.tsx",
    "_layout.tsx",
    "../ui/src/**/*.ts",
    "../ui/src/**/*.tsx",
    "../forms",
    "config/platforms.ts",
    "config/navigationConfig.ts"
, "config/docsLayout.tsx"  ],
  "exclude": [
    "node_modules",
    ".expo",
    "dist",
    "build",
    "examples",
    "examples/**/*"
  ]
}